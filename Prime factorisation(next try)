// learning to find out prime factorization of a number;
// some codes work but why?
#include<iostream>
using namespace std;
int main() {
	int number;
	cout << "Enter a number = " << endl;
	cin >> number;
	for (int i = 2; i <= number; i++) {
		while (number % i == 0) {
			cout << i << endl;
			number = number / i;
		}
	}
}
